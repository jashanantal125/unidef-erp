    "fields": [
        {
            "fieldname": "graduation_result_link",
            "fieldtype": "Data",
            "in_list_view": 1,
            "label": "Result verification link (if applicable)"
        },
        {
            "fieldname": "graduation_university_email",
            "fieldtype": "Data",
            "in_list_view": 1,
            "label": "University domain email id"
        },
        {
            "fieldname": "graduation_docs_verified",
            "fieldtype": "Select",
            "label": "Documents Verified",
            "options": "\nYes\nNo"
        },
        {
            "depends_on": "eval:doc.graduation_docs_verified == 'Yes'",
            "fieldname": "graduation_docs_pdf",
            "fieldtype": "Attach",
            "label": "Upload in Single PDF"
        },
        {
            "default": "Not Accepted",
            "depends_on": "eval:doc.graduation_docs_verified == 'No'",
            "fieldname": "graduation_docs_status",
            "fieldtype": "Data",
            "label": "Status",
            "read_only": 1
        },
        {
            "fieldname": "column_break_backlog",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "graduation_no_of_backlogs",
            "fieldtype": "Select",
            "label": "No of Backlogs",
            "options": "\nBelow 8\nAbove 8"
        },
        {
            "depends_on": "eval:doc.graduation_no_of_backlogs == 'Below 8'",
            "fieldname": "backlog_below_8_verified",
            "fieldtype": "Select",
            "label": "Backlog Verified (Below 8)",
            "options": "\nYes\nNo"
        },
        {
            "depends_on": "eval:doc.backlog_below_8_verified == 'Yes' && doc.graduation_no_of_backlogs == 'Below 8'",
            "fieldname": "backlog_below_8_proof",
            "fieldtype": "Attach",
            "label": "Upload Backlog Proof (Below 8)"
        },
        {
            "default": "Set Reminder to Verify",
            "depends_on": "eval:doc.backlog_below_8_verified == 'No'  && doc.graduation_no_of_backlogs == 'Below 8'",
            "fieldname": "backlog_below_8_status",
            "fieldtype": "Data",
            "label": "Status",
            "read_only": 1
        },
        {
            "depends_on": "eval:doc.graduation_no_of_backlogs == 'Above 8'",
            "fieldname": "backlog_above_8_accepted",
            "fieldtype": "Select",
            "label": "Conditional Offer Accepted?",
            "options": "\nYes\nNo"
        },
        {
            "depends_on": "eval:doc.backlog_above_8_accepted == 'Yes'  && doc.graduation_no_of_backlogs == 'Above 8'",
            "fieldname": "backlog_above_8_verified",
            "fieldtype": "Select",
            "label": "Backlog Verified (Above 8)",
            "options": "\nYes\nNo"
        },
        {
            "depends_on": "eval:doc.backlog_above_8_verified == 'Yes' && doc.graduation_no_of_backlogs == 'Above 8'",
            "fieldname": "backlog_above_8_proof",
            "fieldtype": "Attach",
            "label": "Upload Backlog Proof (Above 8)"
        },
        {
            "default": "Set Reminder to Verify",
            "depends_on": "eval:doc.backlog_above_8_verified == 'No' && doc.graduation_no_of_backlogs == 'Above 8'",
            "fieldname": "backlog_above_8_status",
            "fieldtype": "Data",
            "label": "Status",
            "read_only": 1
        }
